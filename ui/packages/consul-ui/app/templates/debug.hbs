<App class="docs" id="wrapper">
  <:main-nav>
<DocfyOutput as |node|>
  <ul>
    {{#each node.children as |child|}}
          {{#each child.children as |child|}}
                {{#each child.pages as |child|}}
                  <li>
                    <DocfyLink @to={{child.url}}>
                      {{child.title}}
                    </DocfyLink>
                  </li>
                {{/each}}
          {{/each}}
    {{/each}}
  </ul>
</DocfyOutput>

  </:main-nav>
  <:main>
    {{outlet}}
  </:main>
</App>

